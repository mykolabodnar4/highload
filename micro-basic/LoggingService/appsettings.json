{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Application": {
    "MessagesMap" : "logs" 
  },
  "hazelcast": {
    "clusterName": "logging-cluster",
    "networking": {
        "addresses": [
            "host.docker.internal:5701",
            "host.docker.internal:5702",
            "host.docker.internal:5703"
        ]
    }
  }
}
