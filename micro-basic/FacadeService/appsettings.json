{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "broker-1:9092,broker-2:9092,broker-3:9092",
    "Topic": "message-topic"
  },
  "LoggingService": {
    "Urls": [
        "host.docker.internal:8091",
        "host.docker.internal:8092",
        "host.docker.internal:8093"
    ]
  },
  "MessagingService": {
    "Urls" : ["http://host.docker.internal:8084/api/", "http://host.docker.internal:8085/api/"]
  }
}
