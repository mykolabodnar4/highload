{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kafka": {
        "BootstrapServers": "broker-1:9092,broker-2:9092,broker-3:9092",
        "Topic": "message-topic"
    },
    "Application": {
        "MessagesMap": "messages"
    },
    "hazelcast": {
        "clusterName": "logging-cluster",
        "networking": {
            "addresses": [
                "host.docker.internal:5701",
                "host.docker.internal:5702",
                "host.docker.internal:5703"
            ]
        }
    }
}